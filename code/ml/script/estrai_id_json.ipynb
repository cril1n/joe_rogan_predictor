    "import json\n",
    "\n",
    "with open('../data/video_commenters_final.json', 'r') as file:\n",
    "    data = json.load(file)\n",
    "    \n",
    "ids = list(data.keys())\n",
    "\n",
    "for id in ids:\n",
    "    print(id)\n",
    "\n",
    "with open('../data/extracted_ids.txt', 'w') as out_file:\n",
    "    for id in ids:\n",
    "        out_file.write(id + '\\n')"
   ]
  }
